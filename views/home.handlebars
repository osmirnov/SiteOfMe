<!DOCTYPE html>
<html class="no-js">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{profile.name}} - {{{profile.title}}}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link rel="stylesheet" href="public/css/styles.css">
		
        <!--[if lt IE 9]>
            <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
	</head>
    <body>
        <!--[if lt IE 8]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->

		<article class="container">
            <div class="header">
				<div class="photo">
					<img src="{{photo.imageUrl}}" alt="{{photo.text}}" title="{{photo.text}}" />
				</div>
				<div class="profile">
					<div class="profile-name">
						<h1>{{profile.name}}</h1>
						<h4 class="subheader">{{{profile.title}}}</h3>
					</div>
					<div class="profile-info">
						<div class="about">
							<h3>About me</h3>
							<p>
								{{profile.about}}
							</p>
						</div>
						<div class="contacts">
							<h3>Contacts</h3>
							<ul>
								{{#each profile.contacts}}
   								<li><a href="{{url}}" class="{{type}}">{{text}}</a></li>
  								{{/each}}
							</ul>
						</div>
					</div>
				</div>
            </div>
			<div class="main">
				<div class="main-left">
					<div class="experience">
						<h3>Experience</h3>
						{{#each jobs}}
						<section class="job">
							<h4 class="title">{{title}}</h4>
							<h5 class="place">
								<span class="company">{{company}}</span>&nbsp;<span class="country">{{country}}</span>&nbsp;<span class="period">{{period}}</span>
							</h5>
							{{#if description}}
							<div class="description">
								{{description.text}}
								<ul>
									{{#each description.duties}}
									<li>{{this}}</li>
									{{/each}}
								</ul>							
							</div>
							{{/if}}
						</section>
						{{/each}}
					</div>
					<div class="certification">
						<h3>Certificates & Awards</h3>
						{{#each certificates}}
						<section class="certificate">
							<h4 class="title">{{title}}</h4>
							<h5 class="place">
								<span class="company">{{company}}</span>&nbsp;<span class="country">{{country}}</span>&nbsp;<span class="period">{{period}}</span>
							</h5>
							{{#if description}}
							<div class="description">
								{{description.text}}
								<ul>
									{{#each description.duties}}
									<li>{{this}}</li>
									{{/each}}
								</ul>							
							</div>
							{{/if}}
						</section>
						{{/each}}						
					</div>
					<div class="education">
						<h3>Education</h3>
						{{#each schools}}
						<section class="level">
							<h4 class="title">{{title}}</h4>
							<h5 class="place">
								<span class="company">{{company}}</span>&nbsp;<span class="country">{{country}}</span>&nbsp;<span class="period">{{period}}</span>
							</h5>
							{{#if description}}
							<div class="description">
								{{description.text}}
								<ul>
									{{#each description.duties}}
									<li>{{this}}</li>
									{{/each}}
								</ul>							
							</div>
							{{/if}}
						</section>
						{{/each}}
					</div>
				</div>
				<div class="main-right">
					<section class="skills">
						<h3>Skills</h3>
						<ul>
						{{#each skills}}
							<li>
								<div class="name">{{name}}</div>
								<div class="years">
									<span class="years-number">{{years}}</span>&nbsp;year(s)
								</div>
							</li>
						{{/each}}							
						</ul>
					</section>
					<section class="languages">
						<h3>Languages</h3>
						<ul>
						{{#each languages}}
							<li>
								<div class="name">{{name}}</div>
								<div class="level">{{level}}</div>
							</li>
						{{/each}}	
						</ul>					
					</section>
					<section class="publications">
						<h3>Publications</h3>
						<ul>
						{{#each publications}}
							<li>
								<div class="name">{{name}}</div>
							</li>
						{{/each}}	
						</ul>					
					</section>
					<section class="projects">
						<h3>Projects</h3>
						<ul>
						{{#each projects}}
							<li>
								<div class="name">{{name}}</div>
							</li>
						{{/each}}	
						</ul>					
					</section>
				</div>
			</div>
			<div class="footer">
				<section class="customers">
					<h3>Customers</h3>
					<ul>
						{{#each customers}}
							<li>
								<a href="{{url}}" class="customer">
									<img src="{{imageUrl}}" alt="{{text}}" title="{{text}}" />
							 	</a>
							</li>
						{{/each}}	
					</ul>				
				</section>
			</div>
			<div class="copyright">
				<section class="signature">
					{{copyright}}
				</section>
			</div>
		</article>

        <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
        <script>
            (function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
            function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
            e=o.createElement(i);r=o.getElementsByTagName(i)[0];
            e.src='//www.google-analytics.com/analytics.js';
            r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));
            ga('create','{{analyticsId}}');ga('send','pageview');
        </script>
    </body>
</html>
